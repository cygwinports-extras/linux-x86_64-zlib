CROSS_HOST="x86_64-pc-linux-gnu"
ORIG_PN="zlib"
inherit cross

DESCRIPTION="zlib for Linux i686 toolchain"
HOMEPAGE="http://www.zlib.net/"
SRC_URI="mirror://sourceforge/libpng/${ORIG_PN}-${PV}.tar.gz"

src_compile() {
	lndirs
	cd ${B}
	rm -f Makefile
	CHOST=${CROSS_HOST} ./configure --prefix=${CROSS_PREFIX}
	cygmake
}
